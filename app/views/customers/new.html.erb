<% provide(:title, 'New Customer') %>
 <%= render 'application/header' %>
<h1>New Customer</h1>
<%= form_for(@customer) do |f| %>
 
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :email %>
  <%= f.email_field :email %><br>

  <%= f.label :phone_number %>
  <%= f.text_field :phone %><br>

  <%= f.submit %>

<% end %>

