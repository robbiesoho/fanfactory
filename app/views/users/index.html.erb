<% provide(:title, 'All Admins') %>
<%= render 'application/header' %>
<h1>Showing all Admins</h1>
  <div class="all-events-body">
    <table>
      <thead>
        <tr>
          <th>Nombre</th>
          <th colspan="3"></th>
        </tr>
      </thead>

    <% @user.each do |user| %>
      <tr>
        <td><%=user.name%></td>
        <td><%= link_to "delete", user, method: :delete,
                                          data: { confirm: "You sure?" } %></td>

      </tr>
    <% end %>
    </table>
  </div>